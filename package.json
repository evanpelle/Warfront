    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/sinon": "^17.0.3",
    "@types/ws": "^8.5.11",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.1",
    "concurrently": "^8.2.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mocha": "^10.7.0",
    "mrmime": "^2.0.0",
    "sinon": "^18.0.0",
    "sinon-chai": "^4.0.0",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
