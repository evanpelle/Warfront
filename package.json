{
  "name": "warfront-client",
  "scripts": {
    "prebuild": "tsc --project scripts",
    "build-dev": "webpack --config webpack.config.js --mode development",
    "build-prod": "webpack --config webpack.config.js --mode production",
    "start:client": "webpack serve --open --node-env development",
    "start:server": "node --loader ts-node/esm src/server/Server.ts",
    "dev": "concurrently \"npm run start:client\" \"npm run start:server\""
  },
  "devDependencies": {
    "@types/ws": "^8.5.11",
    "concurrently": "^8.2.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.0",
    "mrmime": "^2.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/jimp": "^0.2.28",
    "express": "^4.19.2",
    "jimp": "^0.22.12",
    "node-addon-api": "^8.1.0",
    "node-gyp": "^10.2.0",
    "typia": "^6.5.2",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "type": "module"
}
